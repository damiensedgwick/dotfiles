{
  "buffer_font_family": "ComicShannsMono Nerd Font Mono",
  "buffer_font_size": 17,
  "close_on_file_delete": true,
  "confirm_quit": true,
  "collaboration_panel": {
    "button": false
  },
  "cursor_blink": false,
  "edit_predictions": {
    "mode": "subtle",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null
    },
    "enabled_in_text_threads": false
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "git_panel": {
    "default_width": 300,
    "dock": "right"
  },
  "notification_panel": {
    "button": false
  },
  "outline_panel": {
    "button": false
  },
  "project_panel": {
    "default_width": 300,
    "dock": "right"
  },
  "relative_line_numbers": true,
  "restore_on_startup": "none",
  "ui_font_family": "ComicShannsMono Nerd Font Mono",
  "ui_font_size": 17,
  "vim_mode": true
}
